<% if (typeof errors !== 'undefined') { %>
    <div class="bg-gray-300 shadow py-8 px-4 max-w-md mx-auto">
        <ul>
            <% errors.forEach(error => { %>
                <li class="text-center font-bold text-sm text-white mt-4 block bg-red-600 rouded-lg"><%= error.msg %></li>
            <% }) %>
        </ul>
    </div>
<% } %>


